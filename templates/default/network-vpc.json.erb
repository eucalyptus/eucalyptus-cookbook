{
  "Mode": "VPCMIDO",
  "PublicIps": [
  <% @publicIps.each_with_index do |value, index| -%>
    "<%= value %>"<%= "," unless @publicIps.size == index + 1 %>
  <% end %>
  ],
  "InstanceDnsServers": [
  <% @instanceDnsServers.each_with_index do |value, index| -%>
    "<%= value %>"<%= "," unless @instanceDnsServers.size == index + 1 %>
  <% end %>
  ],
  "Mido": {
    "BgpAsn": <%= @bgpasn %>,
    "Gateways": <%= @mygateways %>
  }
}
